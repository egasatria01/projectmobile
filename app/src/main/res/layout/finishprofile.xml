<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/red">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_kembali"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginRight="360dp"
            android:background="@drawable/rectangle_31"/>


        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ellipse_10" />

        <TextView
            android:id="@+id/tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textSize="16dp"
            android:layout_marginTop="15dp"
            android:text="SELAMAT DATANG NATASYA!!"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textSize="16dp"
            android:layout_marginTop="30dp"
            android:text="Complate Youe Profile"
            android:textColor="@color/white"/>

        <EditText
            android:id="@+id/txt1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/btn_kotak"
            android:ems="10"
            android:textAlignment="center"
            android:hint="Name                          Natasya Giting"
            android:layout_marginRight="40dp"
            android:textStyle="bold"
            android:layout_marginLeft="40dp"
            android:minHeight="48dp"/>
        <EditText
            android:id="@+id/txt2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/btn_kotak"
            android:ems="10"
            android:textAlignment="center"
            android:hint="Number Phone  +62092817363456"
            android:layout_marginRight="40dp"
            android:textStyle="bold"
            android:layout_marginLeft="40dp"
            android:minHeight="48dp"/>
        <EditText
            android:id="@+id/txt3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/btn_kotak"
            android:ems="10"
            android:textAlignment="center"
            android:hint="Email         TasyaSaepul@gmail.com"
            android:layout_marginRight="40dp"
            android:textStyle="bold"
            android:layout_marginLeft="40dp"
            android:minHeight="48dp"/>
        <EditText
            android:id="@+id/txt4"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/btn_kotak"
            android:ems="10"
            android:textAlignment="center"
            android:hint="KTP               -                                       "
            android:layout_marginRight="40dp"
            android:textStyle="bold"
            android:layout_marginLeft="40dp"
            android:minHeight="48dp"/>
        <EditText
            android:id="@+id/txt5"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/btn_kotak"
            android:ems="10"
            android:textAlignment="center"
            android:hint="Address           Gang Harapan blok a1 "
            android:layout_marginRight="40dp"
            android:textStyle="bold"
            android:layout_marginLeft="40dp"
            android:minHeight="48dp"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:background="@drawable/btn_login"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:textColor="@color/white"
            android:text="Logout"/>


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginTop="15dp"
            android:orientation="horizontal"
            android:background="@color/white">

        </ImageView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="-100dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="0dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/group_13" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="0dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/group_14"
                />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="20dp"
                android:layout_height="70dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:background="@drawable/group_24"
                />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="0dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/group_26" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="0dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/group_23" />

        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>